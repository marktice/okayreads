<h1>All Our Books</h1>

<%= link_to 'Add Book', new_book_path %>

<% @books.each do |book| %>
  <ul>
    <li>
      <%= book.title %>
    </li>
    <li>
      <%= book.author %>
    </li>
    <li>
      <%= book.description %>
    </li>
    <li>
      <%= book.genre %>
    </li>
    <li>
      <%= book.year %>
    </li>
  </ul>
<% end %>